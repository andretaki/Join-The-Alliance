    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.0.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@neondatabase/serverless": "^1.0.0",
    "@types/react-signature-canvas": "^1.0.7",
    "@vercel/functions": "^2.2.2",
    "@vercel/kv": "^3.0.0",
    "dotenv": "^17.2.0",
