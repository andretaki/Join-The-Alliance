    "pg": "^8.16.0",
    "postgres": "^3.4.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.4",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "zod": "^3.25.23"
